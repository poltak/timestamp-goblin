{
  "manifest_version": 3,
  "name": "Timestamp Goblin",
  "version": "0.1.0",
  "description": "Persist and restore YouTube watch progress.",
  "permissions": ["storage"],
  "host_permissions": ["https://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
